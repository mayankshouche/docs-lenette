{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Guides",
            "groups": [
              {
                "group": "Get Started",
                "pages": [
                  "introduction",
                  "quickstart",
                  "development",
                  "new-file"
                ]
              },
              {
                "group": "Essentials",
                "pages": [
                  "essentials/markdown",
                  "essentials/code",
                  "essentials/images",
                  "essentials/settings",
                  "essentials/navigation",
                  "essentials/reusable-snippets"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "groups": [
              {
                "group": "API Documentation",
                "pages": [
                  "api-reference/introduction"
                ]
              },
              {
                "group": "Endpoint Examples",
                "pages": [
                  "api-reference/endpoint/get",
                  "api-reference/endpoint/create",
                  "api-reference/endpoint/delete"
                ]
              }
            ]
          }
        ]
      },
      {
        "language": "es",
        "tabs": [
          {
            "tab": "Guías",
            "groups": [
              {
                "group": "Empezar",
                "pages": [
                  "es/introduction",
                  "es/quickstart",
                  "es/development",
                  "es/new-file"
                ]
              },
              {
                "group": "Esenciales",
                "pages": [
                  "es/essentials/markdown",
                  "es/essentials/code",
                  "es/essentials/images",
                  "es/essentials/settings",
                  "es/essentials/navigation",
                  "es/essentials/reusable-snippets"
                ]
              }
            ]
          },
          {
            "tab": "Referencia de API",
            "groups": [
              {
                "group": "Documentación de API",
                "pages": [
                  "es/api-reference/introduction"
                ]
              },
              {
                "group": "Ejemplos de Endpoints",
                "pages": [
                  "es/api-reference/endpoint/get",
                  "es/api-reference/endpoint/create",
                  "es/api-reference/endpoint/delete"
                ]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
